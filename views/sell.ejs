<%- include("partials/header"); -%> 
    <section class="hero" id="home">
        <div class="container">

          <div class="hero-content">

            <!-- form -->

            <h1><center>Upload Flat Details</center></h1>
              <form action = "/sell" method="post">
                  <div class="input-group mb-3">
                    <span class="input-group-text">Flat Description</span>
                    <textarea  placeholder="Enter Flat Description" name="description" class="form-control" aria-label="With textarea"></textarea>
                  </div>
                  <div class="input-group mb-3">
                      <span class="input-group-text">Name</span>
                      <input type="text" placeholder="Enter Full Name" name="fname" aria-label="name" class="form-control">
                    </div>
                    <div class="input-group mb-3">
                      <span class="input-group-text">Contact Details</span>
                      <input name="fcontact" placeholder="Enter Contact Detals" type="text" aria-label="First name" class="form-control">
                    </div>
                    <div class="input-group mb-3">
                      <span class="input-group-text">Location</span>
                      <input placeholder="Enter Location" name="floc" type="text" aria-label="First name" class="form-control">
                      
                    </div>
                    <div class="input-group mb-3">
                      <span class="input-group-text">State</span>
                      <input name="fstate" placeholder="Enter State" type="text" aria-label="First name" class="form-control">
                      
                    </div>
                    <div class="input-group mb-3">
                      <span class="input-group-text">City</span>
                      <input placeholder="Enter City" name="fcity" type="text" aria-label="First name" class="form-control">
                      
                    </div>
                    <div class="input-group mb-3">
                      <span class="input-group-text">Sell/Rent</span>
                      <input placeholder="Enter Sell/Rent" name="ftype" type="text" aria-label="First name" class="form-control">
                      
                    </div>
                    <div class="input-group mb-3">
                      <span class="input-group-text">Furnished</span>
                      <input name="furnish" placeholder="Yes/No" type="text" aria-label="First name" class="form-control">
                      
                    </div>
